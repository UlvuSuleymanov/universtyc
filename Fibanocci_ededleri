#include <iostream>
using namespace std;
int main (){
	int countFibonacciNum;
	int sum=0,num1=0,num2=1;
	
    cout<<"Nece fibonacci ededi gosterilsin? " ;
 	cin>>countFibonacciNum ; 

    for(int i =0;i<countFibonacciNum;i++)
    {   
    	sum=num1+num2;
    	num1=num2;
    	num2=sum;
     cout<<"eded "<<i+1<<" :  "<<sum<<endl;
    	
    	
	}
 	 
     return 0;
}
